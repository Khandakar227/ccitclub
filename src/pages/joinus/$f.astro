---
import globalData from '/assets/global.json';
import Data from '/assets/joinus.json';

import Nav from '../../components/Nav.astro'

const {logo, META} = await globalData;
const {title, description} = await Data;

export async function createCollection() {
    var Forms = await Data?.forms
    return { 
        route: `/joinus/f/:route`,
        paths() {
            return Forms.map((form, i) => ({params: {route: form.route}}))
        },
        async props({params}) {
            return {item: Forms.find((form) => form.route === params.route)}
        },
     };
};
const {item} = Astro.props;
